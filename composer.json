{
  "name": "salopot/image-generator",
  "type": "library",
  "description": "Image provider for Faker: solid color, gradient, gallery, LoremPixel, PicsumPhotos, Unsplash, LoremFlickr, PlaceKitten, PlaceImg",
  "keywords": [
    "faker",
    "image",
    "local",
    "gradient",
    "lorempixel",
    "PicsumPhotos",
    "Unsplash",
    "LoremFlickr",
    "PlaceKitten",
    "PlaceImg"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Alexander Kireev",
      "email": "salopot@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "fakerphp/faker": "^1.13.0",
    "intervention/image": "^2.4"
  },
  "autoload": {
    "psr-4": {
      "Salopot\\ImageGenerator\\": "src"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5||^8.0"
  }
}
